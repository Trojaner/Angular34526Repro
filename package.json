{
  "name": "repro",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve:ssr": "node dist/server.js",
    "build": "ng build --configuration=development",
    "build:production": "ng build --configuration=production --prod --source-map=false",
    "build:ssr": "ng run repro:server:development && npm run webpack:server",
    "build:ssr:production": "ng run repro:server:production --configuration=production && npm run webpack:server",
    "build:full": "npm run build && npm run build:ssr",
    "build:full:production": "npm run build:production && npm run build:ssr:production",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0-rc.7",
    "@angular/cdk": "^9.0.0-rc.6",
    "@angular/common": "9.0.0-rc.7",
    "@angular/compiler": "9.0.0-rc.7",
    "@angular/compiler-cli": "^9.0.0-rc.7",
    "@angular/core": "9.0.0-rc.7",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "9.0.0-rc.7",
    "@angular/localize": "^9.0.0-rc.0",
    "@angular/material": "^9.0.0-rc.6",
    "@angular/platform-browser": "9.0.0-rc.7",
    "@angular/platform-browser-dynamic": "9.0.0-rc.7",
    "@angular/platform-server": "^9.0.0-rc.7",
    "@angular/router": "9.0.0-rc.7",
    "@nguniversal/express-engine": "^9.0.0-next.13",
    "preboot": "7.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.3",
    "ts-loader": "^6.2.1",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.7",
    "@angular/cli": "^9.0.0-rc.7",
    "@angular/compiler-cli": "~9.0.0-rc.7",
    "@angular/language-service": "9.0.0-rc.7",
    "@types/node": "^12.11.1",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "typescript": "~3.6.4",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  }
}
